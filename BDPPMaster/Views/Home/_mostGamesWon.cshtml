@model  List<BDPPMaster.Models.Player>

@{
    List<BDPPMaster.Models.Player> players = Model.OrderByDescending(o => o.gamesWon).ToList();
}
<table class="table table-bordered table-striped">
    <tr>
        <td>User</td>
        <td>Games Won</td>
    </tr>

    @foreach (var player in players)
    {
        <tr>
            <td>@player.screenName</td>
            <td>@player.gamesWon</td>
        </tr>
    }


</table>
